        <script>
        $(document).ready(function () {
           var maxHeight = 0;

           $('body[class*="product-product"] .visible-md').remove();

           $('.product-thumb .caption').each(function () {
              if ($(this).height() > maxHeight) {
                 maxHeight = $(this).height();
              }
           });

           $('.product-thumb .caption').height(maxHeight + 15);
        });
        </script>
      
