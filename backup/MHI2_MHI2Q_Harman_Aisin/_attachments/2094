<script>
  var ada_env = {"env":"prod"};
  var ada_cookie_domain = '.adafruit.com';
    var ada_reduce_motion_cookie = document.cookie.match(/^(.*;)?\s*reduce_motion\s*=\s*1(.*)?$/) !== null;
  var reduceMotion = false;
  // reduce motion if customer set account flag
  if( (typeof ada_reduce_motion !== 'undefined') && ada_reduce_motion === true ){
	  reduceMotion = true;
  }
  // reduce motion if reduce_motion cookie is present
  if( (typeof ada_reduce_motion_cookie !== 'undefined') && ada_reduce_motion_cookie === true ){
	  reduceMotion = true;
  }
  // reduce motion if OS / browser flag is set and available via media query
  var reduceMotionQuery = window.matchMedia('(prefers-reduced-motion: reduce)');
  if( reduceMotionQuery.matches ){
    reduceMotion = true;
  }
  // for use in es6 classes
  window.reduceMotion = reduceMotion;


</script>


